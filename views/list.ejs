<%# // include the header  %>
<%- include("header"); -%>

<%# // main %>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
        <%for(i=0; i<todos.length; i++){%>
        <div class="item">
            <input type="checkbox" class="styled-checkbox" id="styled-checkbox-1" name="check">
            <p><%= todos[i] %></p>
        </div>

        <%}%>  
            <form class="item" action="/" method="post">
                <input  type="text" name="input" placeholder="New item" autocomplete="off" >
                <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
    </div>
 
<%# // include footer %>
<%- include("footer"); -%>